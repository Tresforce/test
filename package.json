{
  "name": "hackathon_starter",
  "version": "1.0.0",
  "description": "quick start repo",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/treforce/hackathon_starter",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "describe": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:bundle": "webpack",
    "local": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "dev": "docker-compose up",
    "stop": "docker-compose down",
    "startLocalDb": "docker run --rm -h localhost --name pg-docker -v $(pwd)/scripts:/docker-entrypoint-initdb.d  -e POSTGRES_PASSWORD=pass -e POSTGRES_USER=ruser -e POSTGRES_MULTIPLE_DATABASES=recipe,testdb -d -p 5433:5432 postgres:11.5",
    "stopLocalDb": "docker kill pg-docker",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:eslint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "jest",
    "test-ci": "run-p test:*",
    "test:build": "tsc --noEmit",
    "test:lint": "eslint \"**/*.{js,ts}\" && prettier \"src/**/*.ts\" --list-different",
    "test:unit": "jest --group=unit --collectCoverage=true",
    "test:integration": "jest --group=integration --collectCoverage=true",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "cov": "codecov",
    "task": "run-s task:*",
    "task:redis": "docker run --rm -h localhost --name redis-docker -d -p 6379:6379 redis",
    "task:run": "ts-node src/services/ErrorQueue/index.ts",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash build task",
    "help": "trash --help",
    "prepare-release": "run-s test doc:html doc:publish"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all un-tracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=12.18"
  },
  "dependencies": {
    "argon2": "^0.26.2",
    "body-parser": "^1.19.0",
    "bullmq": "^1.8.13",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "http-status-codes": "^1.4.0",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "logdna-winston": "^2.3.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@bitjson/npm-scripts-info": "^1.0.0",
    "@bitjson/typedoc": "^0.15.0-0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.12",
    "@types/passport": "^1.0.3",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "codecov": "^3.7.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-runner-groups": "^2.0.1",
    "jest-runner-tsc": "^1.6.0",
    "jsdoc": "^3.6.4",
    "lint-staged": "^10.2.9",
    "newman": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "prettier": "^1.19.1",
    "standard-version": "^6.0.1",
    "supertest": "^4.0.2",
    "trash-cli": "^3.0.0",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.5.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  }
}
